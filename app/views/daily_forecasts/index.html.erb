<%= turbo_stream_from "dailyforecasts" %>
<div class="container">
  <div class="section">
    <div class="row">
    <div class="col s12 center">
        <h4><%=@location.name%></h4>
      </div>
      <div class="col s12">
        <ul class="collection with-header daily-collection">
          <%= turbo_frame_tag "dailyforecasts" do %>
              <%= render @dailyforecasts %>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>